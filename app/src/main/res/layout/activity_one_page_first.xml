<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".forum.page.onepagefirst.OnePageFirstAty">
<RelativeLayout
    android:id="@+id/re"
    android:layout_width="match_parent"
    android:background="@drawable/shape_hs"
    android:layout_height="50dp">

    <ImageView
        android:id="@+id/onePage_ImageView"
        android:layout_width="25dp"
        android:layout_centerVertical="true"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:src="@mipmap/ahlib_nav_icon_back"
        />

<TextView
    android:id="@+id/one_Page_First_Title"
    android:layout_width="wrap_content"
    android:textSize="22dp"
    android:textColor="@android:color/black"
    android:layout_centerInParent="true"

    android:layout_height="wrap_content" />
</RelativeLayout>


    <lanou.amg1.tools.xlistview.XListView
        android:id="@+id/onPageFirstActivcty_listView"
        android:layout_below="@+id/re"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </lanou.amg1.tools.xlistview.XListView>








    <LinearLayout
        android:id="@+id/onePageActivity_LinearLayout"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:clickable="false"
        android:visibility="invisible"
        android:background="#a9060606"
        android:layout_height="match_parent">

    </LinearLayout>






</RelativeLayout>
